{
    "workbench.tree.indent": 15,
    "remote.SSH.showLoginTerminal": true,
    "security.workspace.trust.untrustedFiles": "open",
    "editor.semanticHighlighting.enabled": true,
    "synthwave84.brightness": 1,
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.fontWeight": 500,
    "editor.fontFamily": "Fira Code,Cascadia Mono,MonacoB,Osaka Mono Bold, Consolas, 'Courier New', monospace",
    "editor.fontWeight": 500,
    "editor.fontSize": 15,
    "terminal.integrated.fontFamily": "Fira Code,Cascadia Mono,MonacoB,Osaka Mono Bold, Consolas, 'Courier New', monospace",
    "editor.tokenColorCustomizations": {
        "[GitHub Dark][GitHub Dark Default][GitHub Dark Dimmed]": {
            "numbers": "#6bd5ff",
            "textMateRules": [
                {
                    "scope": "string",
                    "settings": {
                        "foreground": "#6a9fe4"
                    }
                },
                {
                    "scope": "constant.language",
                    "settings": {
                        "foreground": "#71b651"
                    }
                },
                {
                    "scope": "support.type.property-name.json",
                    "settings": {
                        "foreground": "#86cb6e"
                    }
                }
            ]
        },
        "[GitHub Light Default]": {
            "numbers": "#3b81eb",
            "textMateRules": [
                {
                    "scope": "string",
                    "settings": {
                        "foreground": "#416ca5"
                    }
                },
                {
                    "scope": "constant.language",
                    "settings": {
                        "foreground": "#237e02"
                    }
                },
                {
                    "scope": "variable.parameter",
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#c2802a"
                    }
                },
                {
                    "scope": "support.type.property-name.json",
                    "settings": {
                        "foreground": "#1d6902"
                    }
                },
                {
                    // set the colour of the "class"
                    "scope": "entity.name.type.class",
                    "settings": {
                        "foreground": "#843b2d"
                    }
                },
                {
                    // set the colour of module names
                    "scope": "",
                    "settings": {
                        "foreground": "#b19792"
                    }
                }
            ],
        }
    },
    //"editor.rulers": [0],
    "workbench.colorCustomizations": {
